<seelog minlevel="trace">
  <outputs formatid="main">
    <filter levels="trace, debug, info, warn, error, critical">
      <console />
      <buffered size="10000" flushperiod="1000" >
        <rollingfile type="date" datepattern="2006.01.02" filename="log/wing-crontab.log" namemode="prefix" maxrolls="72"/>
      </buffered>
    </filter>
  </outputs>
  <formats>
    <format id="main" format="[%LEVEL] [%Date(2006-01-02 15:04:05.000)] [%Func][%File:%Line] - %Msg%n"/>
  </formats>
</seelog>